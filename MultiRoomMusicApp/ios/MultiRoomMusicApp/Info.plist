			<string>com.iniviv.multiroommusicapp.audio</string>
			<key>UTTypeTagSpecification</key>
			<dict>
				<key>public.filename-extension</key>
				<array>
					<string>mp3</string>
					<string>m4a</string>
					<string>wav</string>
					<string>flac</string>
					<string>aac</string>
					<string>ogg</string>
				</array>
				<key>public.mime-type</key>
				<array>
					<string>audio/mpeg</string>
					<string>audio/mp4</string>
					<string>audio/wav</string>
					<string>audio/flac</string>
					<string>audio/aac</string>
					<string>audio/ogg</string>
				</array>
			</dict>
		</dict>
	</array>
</dict>
</plist>
